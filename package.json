{
  "name": "SiteInspection",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "express": "^4",
    "hana-cli": "^3.202504.1"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "private": true,
  "cds": {
    "requires": {
      "[hybrid]": {
        "auth": {
          "kind": "mocked"
        }
      },
      "db": "hana",
      "auth": {
        "kind": "xsuaa"
      }
    },
    "sql": {
      "native_hana_associations": false
    },
    "server": {
      "body_parser": {
        "limit": "10mb"
      }
    }
  }
}
